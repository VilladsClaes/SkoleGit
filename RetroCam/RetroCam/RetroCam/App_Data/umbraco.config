        <produkt id="1086" key="1ba34067-4ec5-498b-9e23-3f10f4c783e5" parentID="1084" level="4" creatorID="0" sortOrder="1" createDate="2016-09-12T13:05:33" updateDate="2018-05-01T14:00:13" nodeName="Olympus Mju-II" urlName="olympus-mju-ii" path="-1,1054,1071,1084,1086" isDoc="" nodeType="1075" creatorName="Leif Andersen" writerName="Leif Andersen" writerID="0" template="1074" nodeTypeAlias="produkt">
