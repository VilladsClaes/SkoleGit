    <produkter id="1071" key="fa0a15af-4278-4f49-a732-5d3a09a80d1e" parentID="1054" level="2" creatorID="0" sortOrder="0" createDate="2016-09-12T10:54:57" updateDate="2018-05-01T10:21:41" nodeName="Kameraer" urlName="kameraer" path="-1,1054,1071" isDoc="" nodeType="1070" creatorName="Leif Andersen" writerName="Leif Andersen" writerID="0" template="1069" nodeTypeAlias="produkter">
